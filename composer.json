{
    "name": "fabrizio-cafolla/lumen-cache-service",
    "description": "Cache service for Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen", "cache", "redis", "aws-elasticache"],
    "license": "MIT",
    "type": "lumen-package",
    "authors": [
        {
            "name": "Fabrizio Cafolla",
            "email": "info@fabriziocafolla.com"
        }
    ],

    "require": {
        "illuminate/redis": "^5.6",
        "predis/predis": "^1.1"
    },

    "autoload": {
        "psr-4": {
            "LumenCacheService\\": "src/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "LumenCacheService\\CacheServiceProvider"
            ],
            "aliases": {
                "CacheService": "LumenCacheService\\Facades\\CacheService",
                "CacheFile": "LumenCacheService\\Facades\\CacheFile",
                "CacheRedis": "LumenCacheService\\Facades\\CacheRedis"
            }
        }
    }
}
